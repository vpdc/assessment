{
  "name": "empty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "temporal": "temporal server start-dev --ui-port 8080 --db-filename clusterdata.db",
    "ts_node": "node --env-file=.env -r ts-node/register",
    "start": "yarn ts_node src/user-greeting/worker.ts",
    "start.watch": "yarn ts_node --watch src/worker.ts",
    "start.birthday-worker": "TASK_QUEUE=birthday-greetings yarn start",
    "start.anniversary-worker": "TASK_QUEUE=anniversary-greetings yarn start",
    "workflow": "yarn ts_node src/schedule-client.ts",
    "test": "jest"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@temporalio/activity": "^1.13.2",
    "@temporalio/client": "^1.13.2",
    "@temporalio/envconfig": "^1.13.2",
    "@temporalio/worker": "^1.13.2",
    "@temporalio/workflow": "^1.13.2",
    "axios": "^1.13.2",
    "luxon": "^3.7.2",
    "nanoid": "5.1.6",
    "vince-common": "^0.0.14",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@temporalio/nyc-test-coverage": "^1.13.2",
    "@temporalio/testing": "^1.13.2",
    "@tsconfig/node24": "^24.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jest": "^29.2.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
